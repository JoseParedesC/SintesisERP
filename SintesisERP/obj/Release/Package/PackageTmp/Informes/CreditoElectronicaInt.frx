    private void Page1_StartPage(object sender, EventArgs e)
    {                                   
      ReportPage rp = (ReportPage)sender;
      object logoBase64 =(rp.Report.GetColumnValue(&quot;Logo.url&quot;));
      if(logoBase64 != null &amp;&amp; Validcampo(&quot;Logo.url&quot;))
      {                                           
        Image logo = Base64ToImage(logoBase64.ToString());        
        Picture2.Image = logo;
      }
      logoBase64 =(rp.Report.GetColumnValue(&quot;Firma.url&quot;));
      if(logoBase64 != null &amp;&amp; Validcampo(&quot;Firma.url&quot;))
      {                                           
        Image logo = Base64ToImage(logoBase64.ToString());        
        Picture1.Image = logo;
      }
    } 
    
    public bool Validcampo(string campo)
    {
      if(!String.IsNullOrEmpty(((String)Report.GetColumnValue(campo))))          
        return true;
      else
        return false;
    }
    
    private string GetFormattedNumberStandard(object Text, string formato, string region)
    {                                            
      try                              
      {
        decimal DecimalNumber = 0;

        //Válidamos si el texto de entrada es vacio, para ponerlo 0 ó vacio segun el caso
        if (Text == null || Text.ToString() == string.Empty || Text.ToString() == &quot;NaN&quot;) {Text = 0; return &quot;0.00&quot;; }

        //Válidamos si el texto de entrada es un decimal, para poder darle formato.
        if (Text is decimal)
        {
          DecimalNumber = (decimal)Text;
        }
        else
        {     
          DecimalNumber = decimal.Parse(Text.ToString(), CultureInfo.InvariantCulture);     
        }        
        return DecimalNumber.ToString(formato, new CultureInfo(region));               
      }
      catch (Exception)
      {
        return Text.ToString();       
      }                                
    }
    
    public Image Base64ToImage(string base64String)
    {                                     
      byte[] imageBytes = Convert.FromBase64String(base64String);
      MemoryStream StreamDatos = new MemoryStream(imageBytes, 0,imageBytes.Length);
                                    
      StreamDatos.Write(imageBytes, 0, imageBytes.Length);
      Image image = Image.FromStream(StreamDatos, true);
      return image;
    }  
  }
} </ScriptText>
